// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Item {
  id       String    @id @default(cuid())
  name     String    @unique @db.VarChar(255)
  storages Storage[]
}

model Storage {
  id    String @id @default(cuid())
  name  String @unique @db.VarChar(255)
  items Item[]
}
